{
  "name": "tcr-mars-rover",
  "version": "1.0.0",
  "description": "A node skeleton for testing with TDD / TCR",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul cover _mocha",
    "tdd": "mocha -w",
    "tcr": "tcr-cli",
    "test": "mocha --color",
    "commit": "git add --all && git commit -m \"$COMMIT_MSG\" && echo 'commited'",
    "revert": "git checkout HEAD -- src && git reset HEAD src/ -- && git clean -fd && echo 'reverted'",
    "push": "git push"
  },
  "keywords": [
    "tdd",
    "tcr",
    "node",
    "skeleton",
    "test",
    "kentbeck"
  ],
  "author": "Jordi Mart√≠ @itortv",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "tcr-cli": "^1.0.0"
  },
  "dependencies": {}
}
